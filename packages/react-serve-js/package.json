{
  "name": "react-serve-js",
  "version": "0.6.0",
  "description": "React-style backend framework for building APIs with JSX",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "prepublishOnly": "npm run build",
    "dev:serve": "tsx src/dev-runner.tsx",
    "cli:build": "tsc -p tsconfig.json",
    "cli:dev": "tsx src/cli.ts"
  },
  "bin": {
    "react-serve": "dist/cli.js"
  },
  "keywords": [
    "react",
    "backend",
    "framework",
    "jsx",
    "api",
    "express",
    "typescript",
    "cors"
  ],
  "author": "",
  "license": "MIT",
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0"
  },
  "dependencies": {
    "@types/cors": "^2.8.19",
    "commander": "^14.0.0",
    "cors": "^2.8.5",
    "express": "^5.1.0"
  },
  "devDependencies": {
    "@types/express": "^5.0.0",
    "@types/react": "^19.1.10",
    "typescript": "^5.0.0"
  }
}